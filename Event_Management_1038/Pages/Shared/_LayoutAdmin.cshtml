@* 

*@

            <!DOCTYPE html>
            <html lang="en">
            <head>
                <meta charset="utf-8" />
                <meta name="viewport" content="width=device-width, initial-scale=1.0" />
                <title>@ViewData["Title"] - Admin</title>
                <script type="importmap"></script>
                <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
                <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
                <link rel="stylesheet" href="~/Event_Management_1038.styles.css" asp-append-version="true" />
                <style>
                    /* Minimal sidebar styles - move to site.css if needed */
                    .admin-sidebar {
                        min-height: calc(100vh - 136px); /* rough height to reach footer (header+footer combined approx) */
                    }
                    .admin-sidebar .nav-link {
                        color: #333;
                    }
                    .admin-sidebar .nav-link.active,
                    .admin-sidebar .nav-link:focus,
                    .admin-sidebar .nav-link:hover {
                        background-color: #f8f9fa;
                        color: #0000;
                    }

                </style>
            </head>
            <body>
                <header>
                    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
                        <div class="container">
                
                            <img src="data:image/jpeg;base64," alt="Custom Event Logo" style="width: 50px; height: auto;">
                            <a class="navbar-brand" asp-area="" asp-page="/Index">Admin panel</a>

                            <!-- Sidebar toggle button for small screens -->
                            <button class="btn btn-outline-secondary d-md-none ms-2 me-2" type="button" data-bs-toggle="collapse" data-bs-target="#adminSidebar" aria-controls="adminSidebar" aria-expanded="false" aria-label="Toggle admin menu">
                                <span class="navbar-toggler-icon"></span>
                            </button>

                            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                                    aria-expanded="false" aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"></span>
                            </button>
                            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                                <ul class="navbar-nav flex-grow-1">
                                    <li class="nav-item">
                                        <a class="nav-link bg-danger" href="~/Admin/Index">Admin</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                                    </li>
                       
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                                    </li>
                                </ul>
                  
                                <partial name="_LoginPartial" />
                            </div>
                        </div>
                    </nav>
                </header>

                <div class="container">
                    <div class="row">



                        <!-- Admin Sidebar -->
                        <aside id="adminSidebar" class="collapse d-md-block col-12 col-md-3 col-lg-2 bg-light admin-sidebar p-3">
                            <nav class="nav flex-column" aria-label="Admin navigation">
                    <li class="nav-item">
                        <a class="nav-link text-dark"
                           asp-area=""
                           asp-page="/Admin/Event_mgnmnt/Index">
                            Manage Events
                        </a>
                                <a class="nav-link" asp-area="" asp-page="/Admin/Index">Dashboard</a>
                                <a class="nav-link" asp-area="" asp-page="/Admin/Users">Users</a>
                                <a class="nav-link" asp-area="" asp-page="/Index">Back to Site</a>
                                <a class="nav-link" asp-area="" asp-page="/Admin/Events"></a>
                                <a class="nav-link" asp-area="" asp-page="/Admin/Venues"></a>
                                <a class="nav-link" asp-area="" asp-page="/Admin/Settings"></a>
                                <a class="nav-link" asp-area="" asp-page="/Admin/Logs"></a>
                                <hr />
                                
                            </nav>
                        </aside>

                        <!-- Main content column -->
                        <main role="main" class="col-12 col-md-9 col-lg-10 pb-3">
                            @RenderBody()
                        </main>
                    </div>
                </div>

                <footer class="border-top footer text-muted">
                    <div class="container">
                        &copy; 2025 - Event_Management_1038 - <a asp-area="" asp-page="/Privacy">Privacy</a>
                    </div>
                </footer>

                <script src="~/lib/jquery/dist/jquery.min.js"></script>
                <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
                <script src="~/js/site.js" asp-append-version="true"></script>

                @await RenderSectionAsync("Scripts", required: false)
            </body>
            </html>
